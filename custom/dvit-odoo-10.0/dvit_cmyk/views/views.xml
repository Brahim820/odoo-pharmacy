<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="product_form" model="ir.ui.view">
    <field name="name">product_form</field>
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_only_form_view"/>
    <field name="arch" type="xml">
      <xpath expr="//page[@name='general_information']" position="after">
        <page string="CMYK Attributes">
        <group>
          <group string="CMYK Attributes">
            <!-- <field name="seq_field_ids">
              <tree string="Fields">
                <field name="name"/>
              </tree>
            </field> -->
            <field name="business_unit_id"/>
            <field name="bunit_group_id"/>
            <field name="main_group_id"/>
            <field name="type_type_id"/>
            <field name="svc_type_id"/>
            <field name="comm_name_id"/>
            <field name="packing_id"/>
            <field name="color_id"/>
          </group>
        </group>
      </page>
      </xpath>
    </field>
  </record>
</odoo>
